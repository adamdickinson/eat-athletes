{
  "private": true,
  "name": "tracker-portfolio",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest ./tests",
    "relay": "node src/update-schema.js && relay-compiler --src ./src --schema ./schema.graphql",
    "relay:watch": "node src/update-schema.js && relay-compiler --src ./src --schema ./schema.graphql --watch",
    "backend:start": "start-tracker-backend",
    "backend:stop": "stop-tracker-backend"
  },
  "eslintConfig": {
    "rules": {}
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "babel-plugin-relay": "^1.5.0",
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.0",
    "jest": "^21.2.1",
    "preact-cli": "^2.1.0",
    "preact-render-spy": "^1.2.1",
    "relay-compiler": "^1.5.0",
    "style-loader": "^0.20.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "dependencies": {
    "@elevated/tracker-backend": "file:/data/tracker-backend/",
    "@renegade/react-fields": "https://github.com/adamdickinson/react-fields.git#v1.1.0",
    "classnames": "^2.2.5",
    "downloadjs": "^1.4.7",
    "graphql": "^0.13.1",
    "lodash": "^4.17.5",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.6.1",
    "moment": "^2.20.1",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-redux": "^2.0.3",
    "preact-render-to-string": "^3.7.0",
    "preact-router": "^2.5.7",
    "progressbar.js": "^1.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-material-icon-svg": "^1.4.0",
    "react-relay": "^1.5.0",
    "react-sound": "^1.1.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "xlsx-populate": "^1.14.0"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/src/tests/__mocks__/browserMocks.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss|styl)$": "identity-obj-proxy",
      "^./style$": "identity-obj-proxy",
      "^preact$": "<rootDir>/node_modules/preact/dist/preact.min.js",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat",
      "^create-react-class$": "preact-compat/lib/create-react-class",
      "^react-addons-css-transition-group$": "preact-css-transition-group"
    }
  }
}
